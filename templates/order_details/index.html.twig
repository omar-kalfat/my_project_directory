<!DOCTYPE html>

<title>OrderDetails index</title>

{% block body %}
    <h1>OrderDetails index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Type</th>
                <th>Quantity</th>
                <th>Amount</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for order_detail in order_details %}
            <tr>
                <td>{{ order_detail.id }}</td>
                <td>{{ order_detail.type }}</td>
                <td>{{ order_detail.quantity }}</td>
                <td>{{ order_detail.amount }}</td>
                <td>
                    <a href="{{ path('app_order_details_show', {'id': order_detail.id}) }}">show</a>
                    <a href="{{ path('app_order_details_edit', {'id': order_detail.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_order_details_new') }}">Create new</a>
{% endblock %}
